programa {
  funcao inicio() {
    

    real num1 
    escreva("Informe um número: ")
    leia(num1)

    real num2 
    escreva("Informe um número: ")
    leia(num2)

    se(num1 > num2){
      escreva(num1," é maior que ", num2)
    } senao {
      escreva(num2," é maior que ", num1)
    }

  }
}
